<%- include('../partials/header') %>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Post</th>
        </tr>
    </thead>
    <tbody>
        <% posts.forEach(function(post) { %>
            <tr>
                <td><%= post.title %></td>
                <td><%= post.content %></td>
                <td> <a href="/posts/<%= post.id %>">Post Details</a></td>
            </tr>
        <% }) %>
    </tbody>
</table>

<%- include('../partials/footer') %>